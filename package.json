{
  "name": "serialized-query",
  "version": "0.7.2",
  "description": "A simple Typescript class to serialize Firebase queries",
  "license": "MIT",
  "repository": "https://github.com/forest-fire/serialized-query.git",
  "author": "Ken Snyder <ken@ken.net>",
  "keywords": [
    "firebase",
    "typescript"
  ],
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "module": "lib/es/serialized-query.js",
  "main": "lib/cjs/serialized-query.js",
  "typings": "lib/es/serialized-query.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "clean:nuclear": "rimraf lib dist node_modeles && yarn cache clean && rm yarn.lock && yarn && yarn upgrade",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "rm -rf lib && tsc -P tsconfig.esnext.json && tsc -P tsconfig.json",
    "deploy": "ts-node ./scripts/deploy.ts",
    "test": "ts-node ./scripts/test.ts ",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "watch": "ts-node ./scripts/watch.ts",
    "watch:test": "ts-node ./scripts/watch.ts test",
    "watch:all": "ts-node ./scripts/watch.ts all",
    "cli": "ts-node cli/index.ts --"
  },
  "dependencies": {
    "common-types": "^1.11.10"
  },
  "devDependencies": {
    "@firebase/database-types": "^0.4.0",
    "@types/chai": "^4.1.2",
    "@types/chalk": "^2.2.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^6.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.8.5",
    "@types/stack-trace": "^0.0.29",
    "abstracted-admin": "^0.26.3",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "firemock": "^0.27.2",
    "firemodel": "^0.26.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "mocha": "^6.1.4",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "test-console": "^1.1.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
