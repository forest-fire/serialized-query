    "nyc": "^11.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "shelljs": "^0.7.0",
    "test-console": "^1.0.0",
    "ts-node": "^3.1.0",
    "tslint": "^5.0.0",
